,KAAA,CAAA,mBAAA,EAAA;AAAA,KAAA,EAAA,IAAA,CAAA;AACA,IAAA,WAAA,CAAA;AAAA,aAAA,KAAA,CAAA,gBAAA,EAAA;AAAA,KAAA,EAAA,IAAA,CAAA;AACA,GAvCA;AAwCA,EAAA,OAxCA,qBAwCA;AAAA;;AACA,IAAA,KAAA,CACA,GADA,CACA,mGADA,EAEA,IAFA,CAEA,UAAA,QAAA;AAAA,aACA,MAAA,CAAA,KAAA,CAAA,IAAA,GAAA,QAAA,CAAA,IAAA,CAAA,OAAA,CAAA,CAAA,EAAA,IAAA,EACA,MAAA,CAAA,KAAA,CAAA,WAAA,GAAA,IAAA,CAAA,KAAA,CAAA,QAAA,CAAA,IAAA,CAAA,IAAA,CAAA,IAAA,GAAA,MAAA,CADA,EAEA,MAAA,CAAA,KAAA,CAAA,WAAA,GAAA,QAAA,CAAA,IAAA,CAAA,OAAA,CAAA,CAAA,EAAA,WAHA;AAAA,KAFA;AAOA;AAhDA,CAAA","sourcesContent":["<template id=\"template-header\">\n  <div id=\"wrapper-header\">\n    <!-- <div class=\"app-horloge\"> -->\n      <div id=\"time\">\n        {{ horloge  }}\n      </div>\n      <div id=\"date\">\n        {{ date }}\n      </div>\n    <!-- </div> -->\n    <div id=\"weather\">\n      <img id=\"weather-icon\"  v-bind:src=\"imageMeteo\"  v-bind:alt=\"imageMeteo\" />\n      <div id=\"weather-temp\">\n        {{ meteo.temperature }}°C\n      </div>\n\n      <span id=\"weather-description\">\n        {{ meteo.description }}\n      </span>\n    </div>\n    <div id=\"cycleLunaire\">\n        {{ getLunePhase() }} {{ Math.round((getLunePhase() * 35900) / 100)}}\n\n        <p>I'm so lazy!</p>\n      <!-- <div id=\"lune\">\n        <div id=\"discLunaire\"></div>\n      </div> -->\n    </div>\n  </div>\n\n\n</template>\n<script>\nimport moment from 'moment'\nimport axios from 'axios'\nimport lune from 'lune'\n// import { gsap } from \"gsap\";\n\n  export default{\n    data: () => {\n      return {\n        horloge: null,\n        date: null,\n        meteo: {\n          description: null,\n          icon: null,\n          temperature: null,\n        }\n      }\n    },\n    methods:{\n      updateDateActuel(){\n        this.horloge = moment().locale('fr').format('dddd DD MMMM YYYY')\n      },\n      updatehorlogeActuel(){\n        this.date = moment().locale('fr').format('HH:MM:ss')\n      },\n      getLunePhase(){\n        var phaseLune = lune.phase(new Date())\n        var arrondiePhaseLune = Math.round(phaseLune.phase * 100) / 100;\n        return arrondiePhaseLune;\n      }\n    },\n    computed:{\n      imageMeteo(){\n        return require(`@/assets/weather/${this.meteo.icon}.png`)\n      },\n      // angleLunaison(){\n      //   return{\n      //     '--degrésLunaison' : Math.round((getLunePhase() * 35900) / 100)\n      //   }\n      // }\n    },\n\n    created(){\n        setInterval(() => this.updatehorlogeActuel(), 1000);\n        setInterval(() => this.updateDateActuel(), 1000);\n    },\n    mounted () {\n      axios\n      .get('https://api.openweathermap.org/data/2.5/weather?q=Paris,fr&APPID=ec034b22928d18a06a6a16a8352bab42')\n      .then(response => (\n        this.meteo.icon = response.data.weather[0].icon,\n        this.meteo.temperature = Math.round(response.data.main.temp - 273.15),\n        this.meteo.description = response.data.weather[0].description\n      ))\n    },\n\n  }\n\n</script>\n<style lang=\"scss\" scoped>\n// @import '@/assets/_moonPhase';\n\n#wrapper-header{\n  display: grid;\n\n  grid-template-columns: repeat(12, 1fr);\n\n  grid-template-rows: repeat(3, 1fr);\n  grid-gap: 0;\n  // grid-template-columns: 40% 30% 30%;\n  // grid-template-rows: 30% 30% 40%;\n  // background-color: grey;\n\n  // justify-items: stretch;\n\n}\n  #time{\n    color: white;\n    font-weight: 1000;\n    font-size: 1.8rem;\n    font-variant: small-caps;\n    padding: 0.8em 0.2em 0 0.2em;\n\n      align-self: stretch;\n    margin: auto;\n    grid-column: 1 / 6;\n\n    grid-row: 1 / 2;\n    // background-color: purple;\n    // align-items: center;\n    // justify-content: center;\n  }\n    #date{\n      color: white;\n      font-weight: 1000;\n      font-size: 1.5rem;\n      font-variant: small-caps;\n      padding: 0.2em 0.2em 0 0.2em;\n    // background-color: purple;\n      grid-column: 1 / 6;\n\n      grid-row: 2 / 3;\n\n      // hoverflow: hidden;\n    }\n\n\n    #weather{\n\n      grid-column: 6 / 9;\n\n      grid-row: 1 / 3;\n      // background-color: red;\n\n      #weather-icon{\n        filter: invert(100%);\n        width: 5rem;\n        padding-top: 0.5rem;\n        float: left;\n\n        // background-color: blue;\n        // grid-column: 4/5;\n        // background-color: red;\n        // justify-self: stretch;\n        // align-self: stretch;\n\n        // grid-column: 6 / 9;\n        //\n        // grid-row: 1 / 4;\n        // background-color: red;\n      }\n      #weather-temp{\n        // padding: 0.2em 0 0.2em 0;\n        color: white;\n        font-weight: 1000;\n        font-size: 3rem;\n        font-variant: small-caps;\n        padding-top:10px;\n              align-self: stretch;\n        // background-color: green;\n        // grid-column: 6 / 9;\n        //\n        // grid-row: 1 / 4;\n        // background-color: blue;\n      }\n      #weather-description{\n        color: white;\n        font-weight: 1000;\n        font-size: 1.5rem;\n        font-variant: small-caps;\n        // grid-column: 6 / 10;\n        // background-color: green;\n\n              align-self: stretch;\n        // grid-row: 3 / 4;\n      }\n    }\n\n\n      #cycleLunaire{\n\n        grid-column: 9 / 12;\n\n        grid-row: 1 / 4;\n      }\n\n</style>\n"],"sourceRoot":"src/components"}]}