imageMeteo\"  v-bind:alt=\"imageMeteo\" />\n      <div id=\"weather-temp\">\n        {{ meteo.temperature }}Â°C\n      </div>\n\n      <span id=\"weather-description\">\n        {{ meteo.description }}\n      </span>\n    </div>\n    <div id=\"kawaiiGif\">\n\n        <img id=\"gif\" src=\"@/assets/gifKawaii.gif\" alt=\"gifKawaii\">\n        <!-- {{ getLunePhase() }} {{ Math.round((getLunePhase() * 35900) / 100)}}\n\n        <p>I'm so lazy!</p> -->\n    </div>\n  </div>\n\n\n</template>\n<script>\nimport moment from 'moment'\nimport axios from 'axios'\nimport lune from 'lune'\n// import { gsap } from \"gsap\";\n\n  export default{\n    data: () => {\n      return {\n        horloge: null,\n        date: null,\n        meteo: {\n          description: null,\n          icon: null,\n          temperature: null,\n        }\n      }\n    },\n    methods:{\n      updateDateActuel(){\n        this.horloge = moment().locale('us').format(\"MMMM Do YYYY\")\n      },\n      updatehorlogeActuel(){\n        this.date = moment().locale('us').format('h:mm:ss a')\n      },\n      getLunePhase(){\n        var phaseLune = lune.phase(new Date())\n        var arrondiePhaseLune = Math.round(phaseLune.phase * 100) / 100;\n        return arrondiePhaseLune;\n      }\n    },\n    computed:{\n      imageMeteo(){\n        return require(`@/assets/weather/${this.meteo.icon}.png`)\n      },\n    },\n\n    created(){\n        setInterval(() => this.updatehorlogeActuel(), 1000);\n        setInterval(() => this.updateDateActuel(), 1000);\n    },\n    mounted () {\n      axios\n      .get('https://api.openweathermap.org/data/2.5/weather?q=Paris,fr&APPID=ec034b22928d18a06a6a16a8352bab42')\n      .then(response => (\n        this.meteo.icon = response.data.weather[0].icon,\n        this.meteo.temperature = Math.round(response.data.main.temp - 273.15),\n        this.meteo.description = response.data.weather[0].description\n      ))\n    },\n\n  }\n\n</script>\n<style lang=\"scss\" scoped>\n\n#wrapper-header{\n  display: grid;\n  grid-template-columns: repeat(12, 1fr);\n  grid-template-rows: repeat(2, 1fr);\n  grid-gap: 0;\n  // grid-template-columns: 40% 30% 30%;\n  // grid-template-rows: 30% 30% 40%;\n  // background-color: grey;\n\n  // justify-items: stretch;\n\n}\n  #time{\n    color: white;\n    font-weight: 1000;\n    font-size: 1.8rem;\n    font-variant: small-caps;\n    padding: 0.8em 0.2em 0 0.2em;\n    align-self: stretch;\n    margin: auto;\n    grid-column: 1 / 6;\n    grid-row: 1 / 2;\n  }\n    #date{\n      color: white;\n      font-weight: 1000;\n      font-size: 1.5rem;\n      font-variant: small-caps;\n      padding: 0.5em 0.2em 0 0.2em;\n      grid-column: 1 / 6;\n      grid-row: 2 / 3;\n    }\n\n\n    #weather{\n      grid-column: 6 / 9;\n      grid-row: 1 / 3;\n      #weather-icon{\n        filter: invert(100%);\n        width: 5rem;\n        padding-top: 1.3rem;\n        float: left;\n      }\n      #weather-temp{\n        color: white;\n        font-weight: 1000;\n        font-size: 3rem;\n        font-variant: small-caps;\n        padding-top:1.3rem;\n      }\n      #weather-description{\n        color: white;\n        font-weight: 1000;\n        font-size: 1.5rem;\n        font-variant: small-caps;\n        align-self: stretch;\n      }\n    }\n      #kawaiiGif{\n        grid-column: 9 / 13;\n        grid-row: 1 / 3;\n        #gif{\n          width: 100px;\n          overflow: hidden;\n          padding-top: 20px;\n          // margin: auto;\n          // background: blue;\n        }\n      }\n\n</style>\n"]}]}