 data: () => {\n      return {\n        horloge: null,\n        date: null,\n        meteo: {\n          description: null,\n          icon: null,\n          temperature: null,\n        }\n      }\n    },\n    methods:{\n      updateDateActuel(){\n        this.horloge = moment().locale('fr').format('dddd DD MMMM YYYY')\n      },\n      updatehorlogeActuel(){\n        this.date = moment().locale('fr').format('HH:MM:ss')\n      },\n      getLunePhase(){\n        var phaseLune = lune.phase(new Date())\n        var arrondiePhaseLune = Math.round(phaseLune.phase * 100) / 100;\n        return arrondiePhaseLune;\n      }\n    },\n    computed:{\n      imageMeteo(){\n        return require(`@/assets/weather/${this.meteo.icon}.png`)\n      },\n      // angleLunaison(){\n      //   return{\n      //     '--degrésLunaison' : Math.round((getLunePhase() * 35900) / 100)\n      //   }\n      // }\n    },\n\n    created(){\n        setInterval(() => this.updatehorlogeActuel(), 1000);\n        setInterval(() => this.updateDateActuel(), 1000);\n    },\n    mounted () {\n      axios\n      .get('https://api.openweathermap.org/data/2.5/weather?q=Paris,fr&APPID=ec034b22928d18a06a6a16a8352bab42')\n      .then(response => (\n        this.meteo.icon = response.data.weather[0].icon,\n        this.meteo.temperature = Math.round(response.data.main.temp - 273.15),\n        this.meteo.description = response.data.weather[0].description\n      ))\n\n// ,\n//         gsap.from('#app-header', {duration:4, scale: 2})\n\n    },\n\n  }\n\n</script>\n<style lang=\"scss\" scoped>\n@import '@/assets/_moonPhase';\n\n#app-header{\n  display: grid;\n  grid-template-columns: 40% 30% 30%;\n  grid-gap: 0;\n  height: 100%;\n  overflow: hidden;\n  // grid-template-rows: 100px;\n  // align-items: stretch;\n  // align-content: stretch;\n  // justify-items: stretch;\n  // justify-content: stretch;\n\n    .app-horloge{\n      color: white;\n      font-weight: 1000;\n      font-size: 1.5rem;\n      font-variant: small-caps;\n      padding: 0.2em 0.2em 0 0.2em;\n      display: grid;\n      grid-template-columns: (1, 1fr);\n\n      grid-template-rows: 40% 60%;\n\n      // align-items: center;\n      // justify-content: center;\n      }\n      .app-meteo{\n        // grid-column: 4/5;\n        background-color: red;\n        // justify-self: stretch;\n        // align-self: stretch;\n        #weather-icon{\n          width: 50px;\n        }\n      }\n      .cycleLunaire{\n        // grid-column: 6/7;\n        background-color: green;\n        // justify-self: stretch;\n        // align-self: stretch;\n      }\n\n      @media only screen and (max-width : 500px) {\n        .cycleLunaire {\n           display: none;\n        }\n        .apsectsAstrologiques{\n          display: none;\n        }\n      }\n\n\n\n\n\n  }\n</style>\n"]}]}